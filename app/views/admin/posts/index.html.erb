<%= render 'layouts/nav' %>

<%= link_to "ユーザー一覧", admin_users_path %>
<%= link_to "質問一覧", admin_posts_path %>
<br>

<% @posts.each do |post| %>
  <%= attachment_image_tag User.find(post.user_id), :image, :fill, 150, 150, fallback: "no_image.png" %>
  <%= post.title %>
  <%= link_to "削除", admin_post_path(post), method: :delete, data: { confirm: "削除しますか？" } %>
  <br>
<% end %>